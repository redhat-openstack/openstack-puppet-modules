<Plugin rrdcached>
  DaemonAddress "<%= @daemonaddress %>"
  DataDir "<%= @datadir %>"
<% unless @createfiles -%>
  CreateFiles "<%= @createfiles %>"
<% end -%>
<% if @createfilesasync -%>
  CreateFilesAsync "<%= @createfilesasync %>"
<% end -%>
<% if @stepsize -%>
  StepSize "<%= @stepsize %>"
<% end -%>
<% if @heartbeat -%>
  HeartBeat "<%= @heartbeat %>"
<% end -%>
<% if @rrarows -%>
  RRARows "<%= @rrarows %>"
<% end -%>
<% @rratimespan.each do |timespan| -%>
  RRATimespan "<%= timespan %>"
<% end -%>
<% if @xff -%>
  XFF "<%= @xff %>"
<% end -%>
</Plugin>
