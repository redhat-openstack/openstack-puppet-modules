<Plugin network>
<% if @timetolive -%>
    TimeToLive <%= @timetolive %>
<% end -%>
<% if @maxpacketsize -%>
    MaxPacketSize <%= @maxpacketsize %>
<% end -%>
<% if @forward -%>
    Forward "<%= @forward %>"
<% end -%>
<% if @reportstats -%>
    ReportStats "<%= @reportstats %>"
<% end -%>
  
</Plugin>
