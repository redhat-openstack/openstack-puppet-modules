<Plugin curl>
  <Page "<%= plugininstance %>">
    URL "<%= @url %>"
<% if @user -%>
    User "<%= @user %>"
<% end -%>
<% if @password -%>
    Password "<%= @password %>"
<% end -%>
<% if @verifypeer -%>
    VerifyPeer "<%= @verifypeer %>"
<% end -%>
<% if @verifyhost %>
    VerifyHost "<%= @verifyhost %>"
<% end -%>
<% if @cacert -%>
    CACert "<%= @cacert %>"
<% end -%>
<% if @header and @collectd_version and (scope.function_versioncmp([@collectd_version, '5.3']) >= 0) -%>
    Header "<%= @header %>"
<% end -%>
<% if @post and @collectd_version and (scope.function_versioncmp([@collectd_version, '5.3']) >= 0) -%>
    Post "<%= @post %>"
<% end -%>
<% if @measureresponsetime -%>
    MeasureResponseTime "<%= @measureresponsetime %>"
<% end -%>
<%= scope.function_template(["collectd/plugin/match.tpl.erb"]) %>
  </Page>
</Plugin>
