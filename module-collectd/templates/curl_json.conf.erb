# Generated by Puppet
LoadPlugin "curl_json"

<Plugin "curl_json">
  <URL "<%= @url %>">
    Instance "<%= @instance %>"

<% if @user %>
    User "<%= @user %>"
    Password "<%= @password %>"
<% end %>

<% @keys.each do |key,keydata| %>
    <Key "<%= key %>">
      Type "<%= keydata['type'] %>"
    </Key>
<% end -%>

  </URL>
</Plugin>
