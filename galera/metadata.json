  "description": "Install/Configure MariaDB with Galera",
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 2.4.0"},
    {"name":"puppetlabs/mysql","version_requirement":">= 2.3.1"},
    {"name":"puppetlabs/xinetd","version_requirement":">= 1.2.0"}
